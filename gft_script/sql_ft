sudo gdb -ex 'set confirm off' --args x86_64-softmmu/qemu-system-x86_64 \
-drive if=none,id=drive0,cache=none,format=raw,file=/mnt/nfs/sql2.img \
-device virtio-blk,drive=drive0,scsi=off -m 2G -enable-kvm -net tap,ifname=tap2 -net nic,model=virtio,vlan=0,macaddr=ae:ae:00:00:00:22 \
-vga std -chardev socket,id=mon,path=/mnt/nfs/vm2.monitor,server,nowait -mon chardev=mon,id=monitor,mode=readline -gft-id 1 -ft-join-port 5002 -smp 1 
